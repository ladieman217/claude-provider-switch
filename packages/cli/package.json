{
  "name": "claude-provider-switch",
  "version": "0.1.5",
  "description": "CLI + local web UI for switching Claude Code providers",
  "license": "MIT",
  "homepage": "https://github.com/ladieman217/claude-provider-switch#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ladieman217/claude-provider-switch.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/ladieman217/claude-provider-switch/issues"
  },
  "keywords": [
    "claude",
    "anthropic",
    "cli",
    "provider",
    "switcher"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "cps": "dist/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts serve",
    "build": "pnpm -C ../ui run build && tsup && node scripts/copy-ui.mjs",
    "lint": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:http": "CPS_ENABLE_HTTP_TESTS=1 vitest run test/server.http.test.ts",
    "prepack": "pnpm run build",
    "prepublishOnly": "pnpm run lint && pnpm run test"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "supertest": "^7.0.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  }
}
